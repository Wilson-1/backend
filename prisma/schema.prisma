datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma.client.js"
}

model Team {
    id Int @id @default(autoincrement())
    name String @unique
    city String
    foundedYear Int
    stadium String?
    coach String?  
    createdAt DateTime @default(now())
    updatedAt DateTime @default(now())

}